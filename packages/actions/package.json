{
  "name": "wen-actions",
  "version": "1.0.10",
  "description": "The engine behind all the wen tools",
  "module": "dist/esm/index.js",
  "types": "dist/esm/types/src/index.d.ts",
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "private": false,
  "scripts": {
    "format": "rome format ./src",
    "format:fix": "rome format --write ./src",
    "lint": "rome check ./src",
    "lint:fix": "rome check --apply ./src",
    "prebuild": "rm -rf dist",
    "test": "vitest",
    "dev": "rollup -c -w",
    "build": "npm run prebuild && rollup -c"
  },
  "keywords": [
    "react",
    "typescript",
    "rollup",
    "metamask"
  ],
  "author": "Gui Bibeau <g.bibeaulaviolette@gmail.com> (https://www.web3-fullstack.com/)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@rollup/plugin-commonjs": "^22.0.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "18.0.14",
    "@types/rebass": "^4.0.10",
    "@vitest/ui": "latest",
    "autoprefixer": "^10.4.4",
    "babel-loader": "^8.2.5",
    "next": "canary",
    "postcss": "^8.4.12",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^2.75.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rome": "0.7.0-next.f6510d6",
    "typescript": "^4.7.4",
    "vite": "^2.9.9",
    "vitest": "latest"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "dependencies": {
    "valtio": "^1.6.1"
  }
