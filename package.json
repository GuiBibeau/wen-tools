{
  "name": "wen-metamask",
  "version": "1.0.0",
  "description": "A minimalist UI library to connect easily to metamask",
  "license": "MIT",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "format": "turbo run format",
    "format:fix": "turbo run format:fix",
    "prepare": "husky install"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "turbo": "^1.3.1"
  }
}
